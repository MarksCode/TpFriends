{
"manifest_version": 2,
"name":"TagProFriends",
"version":"3.0.5",
"description":"Friends list for TagPro",
"author": "Capernicus",
"permissions":[
    "storage"
],
"content_scripts":[
    {
    "matches": [
            "http://*.koalabeast.com/*"
    ],
    "js": [
            "js/thirdParty/firebase.js",
            "js/thirdParty/firebaseApp.js",
            "js/thirdParty/firebaseAuth.js",
            "js/thirdParty/firebaseDB.js",
            "js/thirdParty/jQuery.min.js" ,
            "js/TagProFriends.js"
    ],
    "css": [
            "css/friends.css"
    ],
    "run_at": "document_end"
    }
],
"web_accessible_resources": [
    "friends.html",
    "friendsContent.html",
    "img/notification.png",
    "img/cogwheel.png",
    "img/cogwheel2.png",
    "img/flairs1.png",
    "img/flairs2.png",
    "img/flairs3.png"
]
}
